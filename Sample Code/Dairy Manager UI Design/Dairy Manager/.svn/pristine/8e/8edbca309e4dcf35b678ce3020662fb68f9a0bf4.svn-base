<wicket:extend>
	<div id="content">
    <div class="bread-crumb">
            	<h1>Financial Management / Expense Search</h1>
            </div>
            <form method="post" wicket:id="expensesearch">
                <div wicket:id="feedback"></div>
                    <div class="addData">
                        <ul>
                            <li>
                                <label>Receipt Number:</label><br />
                                <input type="text" wicket:id="rcptnumber" class="number_effect" />
                            </li>
                            <li>
                                <label>Expense Type:</label><br />
                                <select wicket:id="expenseType" class="dropdown_effect"></select>
                            </li>
                             <li>
                                <label>Receipt Start Date:</label><br />
                                <input type="text" wicket:id="saleDate" class="number_effect" />
                            </li>
                            <li>
                                <label>Receipt End Date:</label><br />
                                <input type="text" wicket:id="saleEndDate" class="number_effect" />
                            </li>   
                              <li>
                                <label>Vendor:</label><br />
                                <select wicket:id="vendor" class="dropdown_effect"></select>
                            </li>                             
                        </ul>
                       
                    </div>
                     <div style="clear:both;"></div>
                            <div  class="newButton"><input type="submit" name="Search" value="Search" class="button" 
                            wicket:id="search" /></div>
            </form>
            <div wicket:id="datawrapper">
            <div>
               <span wicket:id="navigator">[dataview navigator]</span>
             </div>	
             		<div class="containerDiv">
                       <div class="rowDivHeader">
                       		 <div>Voucher Number</div>
                            <div>Voucher Type</div>
                            <div>Vendor Code</div>
                            <div>Voucher Date</div>
                            <div>Quantity</div>
                            <div>Unit Price</div>
                            <div>Amount (in Rs)</div>
                            <div>Remarks</div>
                       		
                       </div>
                    
                       <div class="rowDiv" wicket:id="rows" >
                       			<div><a href="#" wicket:id="expense_edit"><span wicket:id="rcptNumberlbl" /></a></div>                                               
                        <div><span wicket:id="receiptType" /></div>
                    	<div><span wicket:id="vendorCode" /></div>
						<div><span wicket:id="saleDate" /></div>
						<div><span wicket:id="quantity" /></div>
                    	<div><span wicket:id="unitPrice" /></div>
						<div><span wicket:id="amount" /></div>
						<div class="last"><span wicket:id="remarks" /></div>
                       </div>
                    </div>				
                
            </div>					
	</div>
</wicket:extend>