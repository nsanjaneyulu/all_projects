<wicket:extend>
	<div id="content">
		<div wicket:id="yearselection">
			<span wicket:id="yearrepeater"><a wicket:id="year"><span>[</span><span wicket:id="yerlabel" /><span>]</span><span>&nbsp;</span></a>
			</span>
		</div>
		<div>&nbsp;</div>
		<div wicket:id="monthSelection">
			<span wicket:id="monthrepeater"><a wicket:id="month"><span>[</span><span wicket:id="monthlabel" /><span>]</span></a><span>&nbsp;</span>
			</span>
		</div>
		<h2><div wicket:id="feedback"></div></h2> 
		<h1>Revenue List</h1>
		<div wicket:id="reportpanel">
			<div class="list list-Header">
				<div>
					<h2>Receipt Number</h2>
					<h2>Receipt Type</h2>
					<h2>Vendor Code</h2>
					<h2>Sale Date</h2>
					<h2>Quantity</h2>
					<h2>Unit Price</h2>
					<h2>Amount (in Rs)</h2>
					<h2 class="last">Remarks</h2>												
				</div>
				<div wicket:id="rows" style="clear:both;">
					<div class="list-Rows">
						<h3><a href="#" wicket:id="revenue_edit"><span wicket:id="receiptNumber" /></a></h3>
	                    <h3><span wicket:id="receiptType" /></h3>
	                    <h3><span wicket:id="vendorCode" /></h3>
	                    <h3><span wicket:id="saleDate" /></h3>
	                    <h3><span wicket:id="quantity" /></h3>
	                    <h3><span wicket:id="unitPrice" /></h3>
	                    <h3><span wicket:id="amount" /></h3>
	                    <h3 class="last"><span wicket:id="remarks" /></h3>
					</div>
				</div>
			</div>
		</div>
	</div>
</wicket:extend>
