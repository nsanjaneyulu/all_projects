html, body {
margin : 0;
padding : 0;
height:100%;
}
body {
color : #333;
background : #fff;
width : 100%;
font : 73% "lucida sans", "Trebuchet MS", Arial, Helvetica, Tahoma, sans-serif;
}
     /*Top Banner*/
#top {
padding : 0.5em;
height:100px;
background:url(../images/top.jpg) repeat-x !important;
background-size:auto auto !important;
color : #484452;
}
#top h1 {
  color: #FFFFFF;
  font-family: trebuchet ms;
  font-size: 40px;
  font-weight: normal;
  padding: 0;
  position: absolute;
  top: 45px;
}
     /*WRAP*/
#container {
width : 100%;
margin : 0 auto;
line-height: 2em;
background:url(../images/bg1.jpg);
color : #333;
height: auto;
}
#container table {
width:100%;
}
     /*Top Navigation*/ 
#topNav ul { 
text-align: right;
float:right;
padding-bottom: 0px;
padding-top: 10px;
padding-left: 0;
margin-top: 50px;
margin-right:20px;
     /* cancels gap caused by top padding in Opera 7.54 */
margin-left: 0;
background:none;
color:#333333;
font-size:12px;
width: 40%;
font-family:"Times New Roman", Times, serif;
line-height : 18px;
font-weight:bold;
} 
#topNav ul li { 
display: inline;
padding-left: 0;
padding-right: 0;
padding-bottom: 0px;
     /* matches link padding except for left and right */
padding-top: 10px;
font-weight:bold;
} 
#topNav ul li a { 
font:bold 16px Tahoma;
padding-left: 10px;
padding-right: 10px;
padding-bottom: 10px;
padding-top: 10px;
color:#FFFFFF;
text-decoration: none;
border-right: 0px solid #fff;
} 
#topNav ul li a:hover { 
font-weight:bold;
background: #C7AFBD;
color: #333;
} 
#topNav #active {
font-weight:bold; 
border-left: 0px solid #fff;
}  
     /*Left Navigation*/ 
#leftNav { 
float : left;
width :18%;
margin : 1px;
padding :1px 0 0 10px;
background:#fff;
height:auto;
} 
#leftNav p { 
margin : 0 0 1em 0;
} 
#leftNavmenu { 
list-style : none;
float : left;
width : 240px;
margin : 10px 0 20px 0;
padding : 0;
font-size : 0.9em;
} 
#leftNavmenu div.divInfo li a {
font:bold 12px Tahoma;
}
#leftNavmenu ul { 
list-style:none
width : 250px;
margin : 0 0 100px 0;
padding : 0;
font-size:10px;
} 
#leftNavmenu li { 
margin-bottom : 2px;
} 
#leftNavmenu li a { 
font: bold 14px tahoma;
line-height:2em;
background:url(../images/pfeil9.gif) no-repeat left center;
height : 24px;
text-decoration : none;
color:Purple;
padding : 2px 0 0 20px;
border-left : 0 solid #8fb8cc;
} 
#leftNavmenu li a:active
{
background:url(../images/pfeil9.gif) no-repeat left center;
}
#leftNavmenu li a:hover { 
color:Teal;border-left : 0 solid Teal;background:url(../images/pfeil8.gif) no-repeat left center;
} 
     /*Anchor Tag For Left Navigation*/
a {
text-decoration:none;color:#CC3300; outline:none; font-weight:bold;
}
a:hover {
text-decoration:underline;
}
     /*Content*/
#content {
width:78%;
margin:0;
float:left;
background:#fff;
border-left: 1px solid #A662AA;
line-height: 2em;
padding :0;
background:e2e2e2;
background-size:100%;
padding:10px;
min-height:600px;
height:auto !important; 
}
#content p { 
padding : 0 2em 0 2em;
color:#CC3333;
} 
#content h1{
font:normal 20px Tahoma;
}
#content-1 {
margin-left :85%;
margin:0 auto;
border-left : 5px solid #A662AA;
padding :0;
height:100%;
background:url(../images/bodybg.jpg);
background-size:100% 100%;
overflow:hidden;
}
     /*Content  Background Color*/
.contentbgcolor {
background:url(../images/border.jpg) repeat-y 275px 0px;
}
     /*Login Form */
#loginform{
background:#e2f0f9;
width: 400px;
margin:0 auto;
position: relative;
top: 30%;
border: 1px solid #1D9BC4;
}
#loginform legend {
color:#1d9bc4;text-align: center;font: bold 50px/85px Arial;padding:10px; width:91%;
background:url(../images/lock.png) no-repeat 15px 0;
}
#loginform table td {
background:none;text-align:left;line-height: 35px; font-weight:bold; color:#0000CC; font-size:16px;padding-right: 15px;
}
#loginform form {
float: left;margin: 0 auto;text-align: center;
}
input.login {
	width: 75px;
	border: none; 
	background: url(images/logininput.gif); 
	padding: 4px; 
}
#loginform table td.loginsubmit {
float: left;padding: 0;text-align: center;width: 100%;
}
#loginform table td.loginsubmit .button {
margin:0px;background:#1D9BC4;
}
     /* SEARCH BOX */
form { 
width:100%;
margin:0;
}
legend {
border-bottom:1px solid #333; padding:7px; width:95%; margin:5px 0 10px;
}
     /*Page Headings*/
h2 {
margin : 0 0 .5em 0;
font-size : 130%;
color:Teal;
background : transparent;
}
h4 {
margin : 0 0 .5em 0;
font-size : 16px;
color:#663300;
font-weight:bold;
text-align:left;
font-family: Tahoma;
}
     /*Image Actions*/
.post { 
background-color : #eee;
padding : 3px;
margin : 20px 10px 0 5px;
border : 0 dashed #8a795d;
font-size : 80%;
} 
.post .date { 
background : url(images/clock.gif) no-repeat left center;
padding-left : 15px;
margin : 0 15px 0 5px;
} 
.post .comments { 
background : url(images/comment.gif) no-repeat left center;
padding-left : 15px;
margin : 0 15px 0 5px;
} 
.post .readmore { 
background : url(images/document.gif) no-repeat left center;
padding-left : 15px;
margin : 0 15px 0 5px;
} 	
     /*Images Displaying*/
img {
border : none;
}
     /*Buttons*/
.button
{
font-family:sans-serif; 
font-size:large;
background:#5E516F;
border-redius:10px;
box-shadow:1px; 
color:#fff; padding:3px 8px; cursor:pointer; margin:10px 20px 0 0px;
width:4em;
}
.newButton {
margin: 20px auto 0;text-align: center;
}
.submit
{
font-family:sans-serif; 
font-size:large;
font-style:italic; 
background:#CCCCCC none; 
color:#000000; 
width:4em;
}
