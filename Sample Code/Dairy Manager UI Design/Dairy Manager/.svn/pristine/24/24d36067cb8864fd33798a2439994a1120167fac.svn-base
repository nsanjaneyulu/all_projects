<wicket:extend>
	<div id="content">
	<div wicket:id="yearselection">
			<span wicket:id="yearrepeater"><a wicket:id="year"><span
					wicket:id="yerlabel" />
			</a>
			</span>
		</div>
	<h2><div wicket:id="feedback"></div></h2> 
		<h1>Live Stock Promotion List</h1>
	<div wicket:id="reportpanel">
		<div class="list list-Header">
				<div>
					<h2>Live Stock Code</h2>
					<h2>Old State</h2>
					<h2>New State</h2>	
					<h2 class="last">Promotion Date</h2>								
				</div>
				<div wicket:id="rows" style="clear:both;">
				<div class="list-Rows">
					<h3><span wicket:id="liveStockCode" /></h3>
					<h3><span wicket:id="oldState" /></h3>	
					<h3><span wicket:id="newState" /></h3>				
					<h3 class="last"><span wicket:id="promotionDate" /></h3>
				</div>
				</div>
			</div>
		</div>
	</div>
</wicket:extend>
