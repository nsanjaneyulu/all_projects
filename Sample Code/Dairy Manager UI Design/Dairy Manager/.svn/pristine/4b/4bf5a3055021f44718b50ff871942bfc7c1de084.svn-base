<wicket:extend>
	<div id="content">
		<h1>Live Stock Health Details</h1>
        <table class="healthDetails">
        	<tr><td><h1 style="width:100%; color:#660000; clear:both; margin:10px 0 15px 0px; font-size:14px; font-weight:bold;">Live Stock Code : <span wicket:id="code"/></h1></td></tr>
			<tr>
				<td>
		<form wicket:id="wrapper">
		<div wicket:id="feedback"></div>
        <table>
			<tr>
				 <h1 style="width:100%; clear:both; margin:10px 0 15px 0px; font-size:16px; font-weight:bold;">Vaccination Details</h1>
			</tr>
	  <tr>
				<td>
					<form wicket:id="vaccineHealthForm">
						<div wicket:id="feedback"></div>
						<div wicket:id="rowPanel">
							<table width="1050" id="dataTable" class="healthDetails">
								<tr>
								  <th></th>
									<th>Type<font color="#FF0000">*</font></th>
									<th>Last Vaccination Date</th>
									<th>Given Date<font color="#FF0000">*</font></th>
									<th>Next Vaccination Date</th>
									<th>Medicine(s) Name</th>
									<th>Batch No</th>
									<th>Mfg Date</th>
								    <th>Exp Date</th>
					      </tr>
								<span wicket:id="rows">
								<tr>
									<td><input type="checkbox" name="check" id="check"
										wicket:id="vaccinehealthcheck" /></td>
									<td nowrap="nowrap"><select wicket:id="vaccinetype" class="dropdown_effect" maxlength="30">											
									</select></td>
									<td nowrap="nowrap"><input type="text" id="last_vaccination_date"
										 wicket:id="last_vaccination_date" class="date_effect"></td>
									<td nowrap="nowrap"><input type="text" id="given_date"
										 wicket:id="given_date" class="date_effect"></td>
									<td nowrap="nowrap"><input type="text" id="next_vaccination_date"
										 wicket:id="next_vaccination_date" class="date_effect"></td>
									<td nowrap="nowrap"><input type="text"  wicket:id="man_name" class="smalltextfield_effect" maxlength="100"/>
                                    </td>
                                    <td nowrap="nowrap"><input name="batchNo" type="text" id="batchNumber"  wicket:id="batchNumber" 
                                    class="date_effect" maxlength="15"/></td>
									<td nowrap="nowrap"><input name="idnumber" type="text" id="man_date"  wicket:id="man_date" 
                                    class="date_effect"/></td>
									<td nowrap="nowrap"><input name="idnumber" type="text"
										id="expiry_date"  wicket:id="expiry_date" class="date_effect"/> </td>
								</tr>
								</span>
								<tr>
									<td colspan="4" align="left" class="addrow">
                                    <INPUT type="button"
										value="Add Row" class="button" wicket:id="vaccination_add" />
                                       
									<INPUT type="button" value="Delete Row"
										class="button" wicket:id="vaccination_remove" /></td>
								</tr>
							</table>
					  </div>
					</form>
				</td>
			</tr>
            </table>
		<table>
			<tr>
				 <h1 style="width:100%; clear:both; margin:10px 0 15px 0px; font-size:16px; font-weight:bold;">Disease Details</h1>
		  </tr>
			<tr>
				<td>
					<form wicket:id="deceaseForm">
						<div wicket:id="feedback"></div>
						<div wicket:id="rowPanel">
							<table width="1050" class="healthDetails" id="dataTable1">
				  <tr>
									<th width="21"></th>
				    <th width="193">Type<font color="#FF0000">*</font></th>
				    <th width="163">Symptoms</th>
				    <th width="152">Start Date</th>
				    <th width="182">End Date</th>
				    <th width="163">Medicines attempted<font color="#FF0000">*</font></th>
				    <th width="144">Treatment Given By<font color="#FF0000">*</font></th>
				    <th width="163">Remarks</th>
							  </tr>
								<span wicket:id="rows">
								<tr>
									<td><input type="checkbox" name="check" id="check"
										wicket:id="deceasecheck" /></td>
									<td><select wicket:id="deceasetype" class="dropdown_effect" maxlength="150"></select></td>
									<td><textarea wicket:id="symptoms" class="smalltextarea_effect" maxlength="150"></textarea></td>
									<td nowrap="nowrap"><input type="text" id="startdate"
										 wicket:id="startdate" class="date_effect"> </td>
									<td nowrap="nowrap"><input type="text" id="enddate"
										 wicket:id="enddate" class="date_effect"> </td>
									<td><textarea wicket:id="medicine" class="smalltextarea_effect" maxlength="150"></textarea></td>
									<td><input type="text" id="treatmentgiven" 
										wicket:id="treatmentgiven" class="textfield_effect" maxlength="150" /></td>
									<td><textarea wicket:id="remarks" class="smalltextarea_effect" maxlength="150"></textarea></td>
								</tr>
								</span>
								<tr>
								  <td class="addrow">
                                  <INPUT type="button"
										value="Add Row" wicket:id="decease_add" class="button" /></td>
									<td><INPUT type="button" value="Delete Row"
										wicket:id="decease_remove" class="button" /></td>
								</tr>
							</table>
					  </div>
					</form>
				</td>
			</tr>
		</table>
		<table>
			<tr>
				 <h1 style="width:100%; clear:both; margin:10px 0 15px 0px; font-size:16px; font-weight:bold;">De-Worming Details</h1>
			</tr>
			<tr>
				<td>
					<form wicket:id="deWormForm">
						<div wicket:id="feedback"></div>
						<div wicket:id="rowPanel">				
					<table id="dataTable2" class="healthDetails">
						<tr>
							<th></th>
							<th>Type<font color="#FF0000">*</font></th>
							<th>Last De-Worming Date</th>
							<th>Given Date<font color="#FF0000">*</font></th>
							<th>Next De-Worming Date</th>
							<th>Medicine(s) Name</th>
							<th>Batch No</th>
							<th>Mfg Date</th>
							<th>Exp Date</th>
						</tr>
						<span wicket:id="rows">
						<tr>
							<td><input type="checkbox" name="check" wicket:id="wormcheck"/></td>
							<td nowrap="nowrap"><select wicket:id="wormType" class="dropdown_effect" maxlength="30"></select></td>
							<td nowrap="nowrap"><input type="text" id="demo11"  wicket:id="lastDate" class="date_effect"></td>
							<td nowrap="nowrap"><input type="text" id="demo12"  wicket:id="givenDate" class="date_effect"></td>
							<td nowrap="nowrap"><input type="text" id="demo13"  wicket:id="nextDate" class="date_effect"></td>
							<td nowrap="nowrap"><input type="text"  wicket:id="manName" class="smalltextfield_effect"
                             maxlength="100"/></td>
                             <td nowrap="nowrap"><input name="batchNumber" type="text" id="batchNumber"  wicket:id="batchNumber" 
                             class="date_effect" maxlength="100"/></td>	
							<td nowrap="nowrap"><input name="idnumber" type="text" id="demo14"  wicket:id="manDate" class="date_effect"/></td>							
							<td nowrap="nowrap"><input name="idnumber" type="text" id="demo15"  wicket:id="expDate" class="date_effect"/></td>
						</tr>
						</span>
						<tr>
						  <td class="addrow"><input type="button"
								value="Add Row" wicket:id="worm_add" class="button" /></td>
						  <td><INPUT type="button" value="Delete Row" wicket:id="worm_remove" class="button" /></td>
						</tr>
					</table>
					</div></form>
				</td>
			</tr>
		</table>
		<table width="805">
<tr>
				
					<td colspan="3"><input type="button" name="back"
						value="Back" class="button" wicket:id="back"/></td>
				<td colspan="3"><input type="button" name="save" value="Save"
					class="button" wicket:id="save"/></td>
					<td width="91" colspan="3"><input type="button" name="next"
						value="Next" class="button"  wicket:id="next"/></td>
</tr>
		</table>
</form>
        </td>
        </tr>
        </table>
	</div>	
</wicket:extend>