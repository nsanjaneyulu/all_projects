 /* summary Page Close*/
.lastCell {
 	border-right: none;
}
table { 
	width: 100%; 
	border-collapse: collapse; 
	line-height:normal;
}
/* Zebra striping */
/*tr:nth-of-type(odd) { 
  background: #0099FF;
}*/
th { 
	color: #FFF; 
	font-weight: normal; 
	background: #6876a0; /* Old browsers */
	background: -moz-linear-gradient(top,  #6876a0 0%, #62739b 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#6876a0), color-stop(100%,#62739b)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #6876a0 0%,#62739b 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #6876a0 0%,#62739b 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #6876a0 0%,#62739b 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #6876a0 0%,#62739b 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6876a0', endColorstr='#62739b',GradientType=0 ); /* IE6-9 */
border-right: 1px solid #fff;
}
th { 
	padding: 10px 5px;
	text-align: left; 
	font-size: 12px;
	font-weight:normal; 
	border-right: 1px solid #fff;
}
td { 
	padding: 10px 5px;
	border: 1px solid #B1B1B1;
	text-align: left; 
	font-size: 12px;
	font-weight:normal;
	color: #14203C;  
}
.date_effect_SuffestedFeeding {
	border:1px solid #62749c;
	font-size: 15px;
	width: 105px;
	height: 20px;
}
.bread-crumb {
	 width: 100%;
	 height: 40px;
	 font-family:Verdana, Arial, Helvetica, sans-serif;
	background: #6876a0; /* Old browsers */
	background: -moz-linear-gradient(top,  #6876a0 0%, #62739b 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#6876a0), color-stop(100%,#62739b)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #6876a0 0%,#62739b 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #6876a0 0%,#62739b 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #6876a0 0%,#62739b 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #6876a0 0%,#62739b 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6876a0', endColorstr='#62739b',GradientType=0 ); /* IE6-9 */
}
.bread-crumb h1{ 
	margin: 0;
	padding: 5px 0 5px 5px;
	color: #FFFFFF;
	font-size: 13px;
	font-weight: bold;
}