<wicket:extend>
	<div id="content">
	<h2><div wicket:id="feedback"></div></h2> 
		<h1>Vendor List</h1>
		<div class="list list-Header">
                <div>
					<h2>Vendor Code</h2>
					<h2>Vendor Type</h2>
					<h2>Vendor Name</h2>	
					<h2>Contact Person</h2>				
					<h2>Contact No</h2>
					<h2>Alternative No</h2>					
					<h2 style="width:220px;">Address</h2>
					<h2 class="last">Remarks</h2>
				</div>
				<div wicket:id="rows" style="clear:both;">
				<div class="list-Rows">
					<h3 style="height:50px;"><a href="#" wicket:id="vendor_edit"><span wicket:id="vendorCode" /></a></h3>
					<h3 style="height:50px;"><span wicket:id="vendorType" /></h3>
					<h3 style="height:50px;"><span wicket:id="vendorName" /></h3>	
					<h3 style="height:50px;"><span wicket:id="vendorContactPerson" /></h3>				
					<h3 style="height:50px;"><span wicket:id="contactNumber" /></h3>
					<h3 style="height:50px;"><span wicket:id="alternateContactNumber" /></h3>					
					<h3 style="width:226px; height:50px;"><span wicket:id="vendorAddress" /></h3>
					<h3 class="last" style="height:50px;"><span wicket:id="remarks" /></h3>
				</div>
				</div>
			</div>
	</div>
</wicket:extend>
