<wicket:extend>
	<div id="content">
		<h1>Live Stock Search</h1>
            <form method="post" wicket:id="stockSearch">
                <div wicket:id="feedback"></div>
                    <div class="addData">
                        <ul>
                            <li>
                                <label>RFID:</label><br />
                                <input type="text" wicket:id="rfid" class="number_effect" />
                            </li>
                            <li>
                                <label>Live Stock Code:</label><br />
                                <input type="text" id="liveStockCode" wicket:id="code" class="number_effect" />
                            </li>
                            <li>
                                <label>Breed:</label><br />
                                <select wicket:id="breedTypeByBreedType" class="dropdown_effect"></select>
                            </li>
                            <li>
                                <label>Origin:</label><br />
                                <select wicket:id="origin" class="dropdown_effect"></select>
                            </li>
                            <li>
                                <label>Live Stock State:</label><br />
                                <select wicket:id="stateType" class="dropdown_effect"></select>
                            </li>
                            <li>
                                <label>Milking Status:</label><br />
                                <select wicket:id="milkingStatus" class="dropdown_effect"></select>
                            </li>
                            <li>
                                <label>Drying Off:</label><br />
                                <select wicket:id="stopMilkingStatus" class="dropdown_effect"></select>
                            </li>
                            <li>
                                <label>Sex:</label><br />
                                <select wicket:id="sex" class="dropdown_effect"></select>
                            </li>
                        </ul>
                        <div style="clear:both;"></div>
                            <div  class="newButton"><input type="submit" name="Search" value="Search" class="button" 
                            wicket:id="search" /></div>
                    </div>
            </form>
            <div wicket:id="datawrapper">
            <div>
               <span wicket:id="navigator">[dataview navigator]</span>
             </div>					
                <div class="list list-Header">
                    <div>
						<h2>RFID</h2>
                        <h2 >Live Stock Code</h2>
                        <h2>Mother Live Stock Code</h2>                        
                        <h2>Live Stock State</h2>
                        <h2>Breed</h2>
                        <h2>Milking Status</h2>
                        <h2 class="last">Origin</h2>
                    </div>
                <div wicket:id="rows" style="clear:both;">
                    <div class="list-Rows">
						<h3><span wicket:id="lsRfid" /></h3>
                        <h3><a href="#" wicket:id="livestock_edit"><span wicket:id="lsLiveStockCode" /></a></h3>
                        <h3><a href="#" wicket:id="motherlscode"><span wicket:id="lsmotherlbl" /></a></h3>                        
                        <h3><span wicket:id="lsStateType" /></h3>
                        <h3><span wicket:id="lsBreedType" /></h3>
                        <h3><span wicket:id="milkingStatus" /></h3>
                        <h3 class="last"><span wicket:id="originType" /></h3>
                    </div>
                </div>
                </div>
            </div>					
	</div>
</wicket:extend>