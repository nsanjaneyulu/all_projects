<wicket:extend>
	<div id="content">
		<h1>Suggested Feeding</h1>
		<table>
			<tr>
				<td>
					<form wicket:id="suggestedForm">
						<div wicket:id="feedback"></div>
						<div wicket:id="rowPanel">
							<table id="dataTable" class="suggestedFeed">
								<tr>
									<th></th>
									<th>Live Stock State <font color="#FF0000">*</font></th>
									<th>Feed Type <font color="#FF0000">*</font></th>
									<th class="feed-last">Feed Quantity Per Live Stock<font color="#FF0000">*</font></th>
									<!--
									<th>Total Feed Quantity</th>
									-->
								</tr>
								<tr>
									<td colspan="2"> <h1 style="width:100%; clear:both; margin:0 0 0 50px; font-size:14px; 
                                    font-weight:bold;">Select Effective Date</h1></td>
									<td colspan="1" nowrap="nowrap"><input type="text" id="effectiveDate" size="10" 
                                    wicket:id="effectiveDate" class="date_effect"/></td>
									<td colspan="1"><a wicket:id="report">
                                    <img src="images/print.gif" alt="View Report"></a></td>
								</tr>
								<span wicket:id="rows">
									<tr>
										<td><input type="checkbox" name="suggestedfeedcheck"
											id="suggestedfeedcheck" wicket:id="suggestedfeedcheck" />
										</td>
										<td>
											<select wicket:id="state_type" class="dropdown_effect" maxlength="30"></select>
                                            <span wicket:id="livestock_total" class="state_effect"></span>
										</td>
										<td>
											<select wicket:id="feed" class="dropdown_effect" maxlength="30"></select>
										</td>
										<td class="feed-last"><input name="feedquantity" type="text"
											id="breeddesc" wicket:id="feedquantity" class="number_effect" maxlength="15"
                                            style="text-align:right;"/>
										</td>
										
									</tr> </span>
								<tr>
									<td class="buttonBg"><input type="button"
										value="Add Row" wicket:id="suggest_addRow" class="button" />
									</td>
									<td class="buttonBg"><input type="button" value="Delete Row"
										wicket:id="suggest_removeRow" class="button" />
									</td>
									<td class="buttonBg"><input type="button"
										value="Update" wicket:id="suggest_update" class="button" />
									</td>
								</tr>
							</table>
						</div>
					</form></td>
			</tr>
			<tr>
				<td>
				</td>
			</tr>
		</table>
	</div>
</wicket:extend>